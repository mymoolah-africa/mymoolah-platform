{
  "name": "mymoolah-portal-backend",
  "version": "1.0.0",
  "description": "MyMoolah Portal Backend - API server for MMAP system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.37.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.53.0",
    "sequelize-cli": "^6.6.2"
  },
  "keywords": [
    "mymoolah",
    "portal-backend",
    "fintech",
    "banking",
    "mojaloop",
    "api",
    "express",
    "postgresql"
  ],
  "author": "MyMoolah Digital Solutions",
  "license": "MIT",
  "engines": {
    "node": ">=18.20.8",
    "npm": ">=9.0.0"
  }
}
